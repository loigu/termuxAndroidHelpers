.userBanner.userBanner--blue { color: rgb(255, 255, 255); background: rgb(0, 8, 227); border-color: rgb(0, 6, 176); }

.userBanner.userBanner--royalBlue { color: rgb(255, 255, 255); background: rgb(65, 105, 225); border-color: rgb(33, 76, 206); }

.userBanner.userBanner--skyBlue { color: rgb(255, 255, 255); background: rgb(124, 195, 224); border-color: rgb(83, 176, 214); }

.userBanner.userBanner--gray { color: rgb(255, 255, 255); background: rgb(128, 128, 128); border-color: rgb(103, 103, 103); }

.userBanner.userBanner--silver { color: rgb(0, 0, 0); background: rgb(192, 192, 192); border-color: rgb(167, 167, 167); }

.userBanner.userBanner--yellow { color: rgb(0, 0, 0); background: rgb(255, 255, 145); border-color: rgb(230, 230, 135); }

.userBanner.userBanner--orange { color: rgb(0, 0, 0); background: rgb(255, 203, 0); border-color: rgb(204, 162, 0); }

.alert.is-unread { color: rgb(186, 184, 184); background: rgb(31, 30, 30); }

.alertToggler { padding: 6px; margin-right: -6px; text-decoration: none !important; }

.alert .alertToggler { opacity: 0; }

.alert:hover .alertToggler, .has-touchevents .alertToggler { opacity: 1; }

.alertToggler-icon { font-size: 0.75em; font-weight: 400; }

.is-unread .alertToggler-icon { font-weight: 900; }

.contentVote { position: relative; display: flex; flex-direction: column; align-items: center; min-width: 1.41em; color: rgb(118, 116, 116); font-size: 24px; }

.contentVote-vote { color: inherit; text-decoration: none; line-height: 1; padding: 6px; transition: all 0.25s ease 0s; }

.contentVote-vote:hover { color: inherit; text-decoration: none; }

.contentVote-vote::before { font-family: "Font Awesome 5 Pro"; font-size: inherit; font-style: normal; font-weight: 400; text-rendering: auto; -webkit-font-smoothing: antialiased; }

.contentVote-vote.is-disabled { opacity: 0.2; cursor: not-allowed; }

.contentVote.is-voted .contentVote-vote:not(.is-voted) { opacity: 0.5; }

.contentVote-vote.is-voted { transform: scale(1.75); }

.contentVote-vote.contentVote-vote--up.is-voted { color: rgb(23, 23, 23); }

.contentVote-vote.contentVote-vote--up::before { content: ""; width: 1.28571em; display: inline-block; text-align: center; }

.contentVote-vote.contentVote-vote--down.is-voted { color: rgb(23, 23, 23); }

.contentVote-vote.contentVote-vote--down::before { content: ""; width: 1.28571em; display: inline-block; text-align: center; }

.message .contentVote-score:first-child { padding-top: 6px; }

.contentVote.is-voted .contentVote-score { color: rgb(23, 23, 23); }

.contentVote-description { font-size: 17px; }

.contentVote.contentVote--small { font-size: 15px; }

.contentVote.contentVote--small .contentVote-description { font-size: 12px; }

.contentVote.contentVote--placeholder .contentVote-score { line-height: 1; }

@media (max-width: 650px) {
  .message .contentVote { flex-direction: row; justify-content: center; }
  .message .contentVote-score { min-width: 1.41em; text-align: center; padding: 0px 6px; }
  .message .contentVote-score:first-child { padding-top: 0px; }
}
